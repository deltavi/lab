{
  "description": "Firefox Webextensions - JavaScript Experiment",
  "manifest_version": 2,
  "name": "JavaScript Experiment",
  "version": "1.0",
  "homepage_url": "http://www.vincenzodevivo.com",
  "icons": {
    "48": "icons/my_lab48.png"
  },
  "browser_action": {
    "default_icon": "icons/my_lab32.png",
